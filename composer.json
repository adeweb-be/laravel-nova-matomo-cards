{
    "name": "adeweb/laravel-nova-matomo-cards",
    "description": "",
    "keywords": [
        "laravel nova",
        "matomo",
        "cards",
        "nova",
        "laravel-nova-matomo-cards"
    ],
    "homepage": "https://github.com/adeweb-be/laravel-nova-matomo-cards",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine DuchÃªne",
            "email": "antoineduchene@icloud.com",
            "homepage": "https://adeweb.be",
            "role": "Lead-Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "thijssimonis/nova-list-card": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Adeweb\\NovaMatomoCards\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adeweb\\NovaMatomoCards\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adeweb\\NovaMatomoCards\\NovaMatomoCardsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
